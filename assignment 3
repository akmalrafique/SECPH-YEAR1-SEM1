#include<iostream>
#include<fstream>
#include<iomanip>
using namespace std;
int main(){
const int size=20
char student[size];
int intake[size],enrollement[size],output[size];
int totalintake=0,totalenroll=0,totaloutput=0;
getInput(.......);
calTotal(intake,size);
calTotal(enrollement,size);
calTotal(output,size);
}
void getInput(STUDENT[],INTAKE[],ENROLLMENT[],OUTPUT[]){
int i=0;
ifstream infile("input.txt");
if(!infile){
cout<<"fail to open the file";
return 0;
}
while(infile>>STUDENT[i]>>INTAKE[i]>>ENROLLMENT[i]>>OUTPUT[i]){
i++;
}
}
int calTotal(){

for(int a=0;a<size;a++){
totalintake+=intake[a];
totalenroll+=enroll[a];
totaloutput+=output[a];
return totalintake,totalenroll,totaloutput;
}
}
